{
  "name": "rehype-pretty-code",
  "version": "0.1.0",
  "description": "A Rehype plugin to make the code your docs beautiful.",
  "main": "./dist/rehype-pretty-code.cjs",
  "module": "./dist/rehype-pretty-code.js",
  "workspaces": [
    "./website"
  ],
  "type": "module",
  "types": "index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/rehype-pretty-code.js",
      "require": "./dist/rehype-pretty-code.cjs"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist/",
    "index.d.ts"
  ],
  "scripts": {
    "build": "npx rollup -c",
    "test": "node --experimental-vm-modules ./node_modules/jest/bin/jest.js"
  },
  "jest": {
    "transform": {}
  },
  "engines": {
    "node": "^12.16.0 || >=13.2.0"
  },
  "author": "",
  "license": "MIT",
  "repository": "github:atomiks/rehype-pretty-code",
  "homepage": "https://rehype-pretty-code.netlify.app",
  "prettier": {
    "bracketSpacing": false,
    "singleQuote": true,
    "proseWrap": "always"
  },
  "peerDependencies": {
    "shiki": "*"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.1.1",
    "hast-util-to-html": "^8.0.3",
    "jest": "^27.4.5",
    "jest-file-snapshot": "^0.5.0",
    "mdast-util-to-hast": "^12.1.0",
    "prettier": "^2.5.1",
    "rehype-parse": "^8.0.3",
    "remark": "^14.0.2",
    "rollup": "^2.60.2",
    "shiki": "^0.9.14",
    "typescript": "^4.5.2",
    "unified": "^10.1.1",
    "unist-util-visit": "^4.0.0"
  },
  "dependencies": {
    "parse-numeric-range": "^1.3.0"
  }
}
